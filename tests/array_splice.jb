{
  var arr = [];
  arr.splice(0, 0, 2, 3);
  assert(arr == [2, 3]);
}
{
  var arr = [2, 3, 4];
  arr.splice(2);
  assert(arr == [2, 3]);
}
{
  var arr = [2, 3, 4];
  arr.splice(0, 1, 5);
  assert(arr == [5, 3, 4]);
}

{
  var arr = [2, 3, 4];
  arr.splice(1, 1, 5);
  assert(arr == [2, 5, 4]);
}
{
  var arr = [2, 3, 4];
  arr.splice(1, 1, 5, 6);
  assert(arr == [2, 5, 6, 4]);
}
{
  var arr = [2, 3, 4, 5];
  arr.splice(1, 2, 6);
  assert(arr == [2, 6, 5]);
}
{
  var arr = [2, 3, 4];
  arr.splice(1, 2, 6);
  assert(arr == [2, 6]);
}

{
  var arr = [2, 3, 4];
  arr.splice(2, 1, 5);
  assert(arr == [2, 3, 5]);
}
{
  var arr = [2, 3, 4];
  arr.splice(2, 1, 5, 6);
  assert(arr == [2, 3, 5, 6]);
}
{
  var arr = [2, 3, 4, 5];
  arr.splice(2, 2, 7);
  assert(arr == [2, 3, 7]);
}
{
  var arr = [2, 3, 4];
  arr.splice(3, 0, 5, 6);
  assert(arr == [2, 3, 4, 5, 6]);
}
